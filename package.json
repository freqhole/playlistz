{
  "name": "playlistz",
  "version": "1.0.0",
  "description": "make some music playlistz in yr browser!",
  "type": "module",
  "scripts": {
    "dev": "vite --config config/vite.config.ts",
    "start": "vite --config config/vite.config.ts",
    "build": "vite build --config config/vite.config.ts",
    "build:standalone": "node build-component.js",
    "preview": "vite preview --config config/vite.config.ts",
    "test": "vitest --run --config config/vitest.config.ts",
    "test:watch": "vitest --config config/vitest.config.ts",
    "test:coverage": "vitest --coverage --run --config config/vitest.config.ts",
    "lint": "eslint",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "music-player",
    "playlist",
    "solid-js",
    "web-component",
    "indexeddb",
    "pwa"
  ],
  "author": "Edward",
  "license": "MIT",
  "dependencies": {
    "idb": "^8.0.3",
    "jszip": "^3.10.1",
    "solid-js": "^1.8.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@solidjs/testing-library": "^0.8.10",
    "@tailwindcss/vite": "^4.1.11",
    "@types/jsdom": "^21.1.7",
    "@types/jszip": "^3.4.0",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-solid": "^0.14.5",
    "fake-indexeddb": "^6.0.1",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.1.0",
    "solid-element": "^1.9.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.3.2",
    "vite": "^6.3.5",
    "vite-plugin-solid": "^2.11.6",
    "vitest": "^1.6.1",
    "vitest-dom": "^0.1.1"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 80,
    "tabWidth": 2,
    "jsxSingleQuote": false
  }
}
